local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("DrifterX Hub", "BloodTheme")

local Tab = Window:NewTab("Free Characters")

local Section = Tab:NewSection("All Free Characters")

Section:NewButton("Wednesday", "Wednesday", function()
    local args = {
    [1] = game:GetService("ReplicatedStorage").SelectACharacter.Wednesday,
    [2] = game:GetService("Players").LocalPlayer.PlayerGui.Menu.Frame.Select.SelectScript
}

game:GetService("ReplicatedStorage").SelectACharacter.ChangeChar:FireServer(unpack(args))
end)

Section:NewButton("Alex", "Alex", function()
    local args = {
    [1] = game:GetService("ReplicatedStorage").SelectACharacter.Steve,
    [2] = game:GetService("Players").LocalPlayer.PlayerGui.Menu.Frame.Select.SelectScript
}

